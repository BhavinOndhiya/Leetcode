class Solution {
public:
    int search(vector<int>& nums, int target) {
        int start = 0;
    int end = nums.size()-1;

    int mid = start + (end-start)/2;

    while(start <= end) {

        if(nums[mid] == target) {
            return mid;
        }

        //go to right wala part
        if(target > nums[mid]) {
            start = mid + 1;
        }
        else{ //key < arr[mid]
            end = mid - 1;
        }

        mid = start + (end-start)/2;
    }
    
    return -1;
    }
};
